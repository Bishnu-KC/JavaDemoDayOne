package com.company.controlflow;

public class IfElseDemo {
	public static void main (String args[])
	{
		/*
		 * 1. if statement
		 * 2. nested if statement
		 * 3. if-else statement
		 * 4. if else-if statement
		 */
		// 1. if statement
		/*
		 * if(condition)
		 * {
		 * statements;
		 * }
		 */
		int a= 10;
		if(a>5)
		{
			System.out.println(a);
		}
		System.out.println("Thankyou, Try next time");
	}

}
