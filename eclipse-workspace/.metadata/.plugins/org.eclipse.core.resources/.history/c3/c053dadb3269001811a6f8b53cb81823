package com.company.selfexample;

public class LoopDemo {
	 public static void main(String args[])
	 {
         int i=10;
         while(i>1)
         {
              System.out.println(i);
              i--;
         }
         int arr[]={2,11,45,9};
         //i starts with 0 as array index starts with 0 too
         int a=0;
         while(a<4){
              System.out.println(arr[a]);
              a--;
         }
    }

}
