package com.company.loopDemo;

public class FibboSeries {
	public static void main(String args[])
	{
		int a,b,c,count;
		a=0;
		b=1;
		count=7;
		//System.out.print(a+"\t"+b);
		for(int i=2; i<=count;i++)
		{
			System.out.println(a+"");
			c=a+b;
			System.out.print("\t"+c);
			a=b;
			b=c;
		}
		
	}

}
