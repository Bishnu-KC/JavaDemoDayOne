package com.method.overloading;
class Test1 {
	  void meth(int i, int j) {
	    i *= 2;
	j /= 2; }
	}
public class CallByValue {
	public static void main(String args[]) {
	    Test1 ob = new Test1();
	    //int a = 15, b = 20;
	    System.out.println("a and b before call: " +
	                       10 + " " + 20);
	    ob.meth(10, 20);
	    System.out.println("a and b after call: " +
	                       10 + " " + 20);
	}

}
